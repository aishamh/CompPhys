    "lx = 8.\n",
    "ly = 8.\n",
    "N = 12\n",
    "dt = 0.02\n",
    "v0 = 1.\n",
    "\n",
    "labels = ['Kinetic Energy', 'Pot Energy', 'Tot Energy']\n",
    "\n",
    "S = MDsystem(lx, ly, N, dt, BoundaryCondition.RBC)\n",
    "S.init(StartConf.Rectangular, v0);\n",
    "kins = []\n",
    "pes = []\n",
    "totE = []\n",
    "\n",
    "for i in range(1000):\n",
    "    S.evolve()\n",
    "    kin = S.kinetic_energy();\n",
    "    pe = S.pot_energy();\n",
    "    tote = S.total_energy();\n",
    "    kins.append(kin)\n",
    "    pes.append(pe)\n",
    "    totE.append(tote)\n",
    "\n",
    "#print kins\n",
    "pyplot.plot(kins, label=labels[0]);\n",
    "pyplot.plot(pes, label=labels[1]);\n",
    "pyplot.plot(totE, label=labels[2]);\n",
    "pyplot.legend(loc='upper right');\n",
    "\n",
    "print \"As it can be seen, total energy is conserved with some round off errors\"\n",
    "\n",
