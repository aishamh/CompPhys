    "nsteps = 50000\n",
    "v_his = np.zeros(nsteps*N)\n",
    "vx_his = np.zeros(nsteps*N)\n",
    "vy_his = np.zeros(nsteps*N)\n",
    "\n",
    "S.init(StartConf.Rectangular, v0)\n",
    "\n",
    "for i in range(1000): # warmup for equilibration\n",
    "    S.evolve()\n",
    "\n",
    "j = 0    \n",
    "for i in range(nsteps):\n",
    "    S.evolve()\n",
    "    for n in range(S.N):\n",
    "        p = S.particles[n]\n",
    "        v = p.vx*p.vx+p.vy*p.vy\n",
    "        v_his[j] += math.sqrt(v)\n",
    "        vx_his[j] += p.vx\n",
    "        vy_his[j] += p.vy\n",
    "\n",
    "        j += 1\n",
    "\n",
    "pyplot.hist(v_his);\n",
    "#print v_his"
   ]
